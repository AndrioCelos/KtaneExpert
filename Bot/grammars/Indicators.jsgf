#JSGF V1.0 UTF-8 en;
grammar Indicators;
import <main.*>;
import <util.*>;

public <root> = (
	<command>
	| [there are] (none|no indicators)
	| done
	| [there is] (a|an) (lit|unlit) <label>
);

<label> = <letter> <letter> <letter>
	| send | sound | clear | car | i n d | india | frequency | sig | turn | bob | freak;
